{
    "name": "xpand/gameshow",
    "version": "1.0.0",
    "description": "An application to play an online gameshow using twitch-chat integration.",
    "keywords": [
        "framework",
        "laravel",
        "gameshow"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Eric Heinzl",
            "email": "xpand.4beatz@gmail.com",
            "role": "Developer",
            "homepage": "https://xpand4b.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.7",
        "lorisleiva/laravel-actions": "^2.3",
        "pusher/pusher-php-server": "^7.0",
        "xpand4b/laravel-command-collection": "@dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "brianium/paratest": "^6.3",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.10",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:xPand4B/Laravel-Command-Collection.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "setup": [
            "composer install --prefer-dist --optimize-autoloader --no-progress",
            "@php artisan app:install"
        ],
        "test": "@php artisan test --parallel"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
